{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "lib",
  "projectType": "library",
  "generators": {
    "defaultConfiguration": "@nx/angular",
    "@nx/angular:component": {
      "type": "component",
      "standalone": true,
      "changeDetection": "OnPush",
      "skipTests": true,
      "inlineStyle": true,
      "inlineTemplate": true
    },
    "@nx/angular:service": {
      "type": "service",
      "skipTests": true
    },
    "@nx/angular:directive": {
      "type": "directive",
      "standalone": true,
      "skipTests": true
    },
    "@nx/angular:pipe": {
      "typeSeparator": ".",
      "standalone": true,
      "skipTests": true
    },
    "@nx/angular:guard": {
      "typeSeparator": "."
    },
    "@nx/angular:interceptor": {
      "typeSeparator": "."
    },
    "@nx/angular:resolver": {
      "typeSeparator": "."
    }
  },
  "sourceRoot": "projects/lib/src",
  "prefix": "daisy",
  "targets": {
    "build": {
      "executor": "@angular/build:ng-packagr",
      "configurations": {
        "production": {
          "tsConfig": "projects/lib/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "projects/lib/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@angular/build:karma",
      "options": {
        "tsConfig": "projects/lib/tsconfig.spec.json"
      }
    }
  }
}